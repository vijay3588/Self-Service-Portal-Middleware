      }
    ],
    [
      {
        "type": "select",
        "name": "existingAddress",
        "label": "Select an Existing Address",
        "placement": "col-md-5 col-12",
        "validations": {}
      },
      {
        "type": "checkbox",
        "name": "createNewAddress",
        "label": "Create New Address",
        "placement": "col-md-5 col-12"
      }
    ],
    [
      {
        "type": "panelSideHeader",
        "label": "Mailing Address"
