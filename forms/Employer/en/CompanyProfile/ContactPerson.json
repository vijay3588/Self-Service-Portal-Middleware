          "string": {
            "required": "mandatory field"
          }
        }
      },
      {
        "type": "select",
        "name": "state",
        "label": "State",
        "placement": "col-md-2 col-12",
        "options": [
          { "value": "ca", "text": "California" },
          { "value": "mn", "text": "Minnesota" },
          { "value": "nj", "text": "New Jersey" }
        ],
        "validations": {
          "string": {
            "required": "mandatory field"
          }
        }
      },
      {
        "type": "text",
        "name": "zipCode",
        "label": "Zip Code",
        "placement": "col-md-2 col-12",
        "validations": {
          "string": {
            "required": "mandatory field"
          }
        }
      },
      {
        "type": "select",
        "name": "country",
        "label": "Country",
        "value": "USA",
        "placement": "col-md-4 col-12",
        "options": [
          { "value": "us", "text": "United States" },
          { "value": "ca", "text": "Canada" },
          { "value": "mx", "text": "Mexico" }
        ],
        "validations": {
          "string": {
            "required": "mandatory field"
          }
        }
      }
    ],
    [
      {
        "type": "panelSideHeader",
        "label": "Phone and Email Information"
      }
    ],
    [
      {
        "type": "text",
        "name": "phoneNumberPrimary",
        "label": "Phone Number (Primary)",
        "placement": "col-md-4 col-12"
      },
      {
        "type": "text",
        "name": "extPrimary",
        "label": "Ext",
        "placement": "col-md-2 col-12"
      },
      {
        "type": "text",
        "name": "phoneNumberSecondary",
        "label": "Phone Number (Secondary)",
        "placement": "col-md-4 col-12"
      },
      {
        "type": "text",
        "name": "extSecondary",
        "label": "Ext",
        "placement": "col-md-2 col-12"
      }
    ],
    [
      {
        "type": "text",
        "name": "faxNumber",
        "label": "Fax Number",
        "placement": "col-md-4 col-12"
      },
      {
        "type": "email",
        "name": "email",
        "label": "Email",
        "placement": "col-md-4 col-12"
      },
      {
        "type": "select",
        "name": "preferredContactMethod",
        "label": "Preferred Contact Method",
        "placement": "col-md-4 col-12",
        "options": [
          { "value": "Mail Only", "text": "Mail Only" },
          { "value": "Email Only", "text": "Email Only" },
          { "value": "Mail and Email", "text": "Mail and Email" }
        ]
      }
    ]
  ]
}